[project]
title = FileDrop
dependencies#0 = https://github.com/britzl/defold-png/archive/refs/tags/1.2.2.zip

[bootstrap]
main_collection = /example/example.collectionc

[input]
game_binding = /input/game.input_bindingc

[display]
width = 1136
height = 640

[script]
shared_state = 1

[library]
include_dirs = filedrop

[collection_proxy]
max_count = 9

[html5]
scale_mode = downscale_fit
show_fullscreen_button = 0
show_made_with_defold = 0
htmlfile = /builtins/manifests/web/engine_template.html

